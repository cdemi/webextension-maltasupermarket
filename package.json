{
    "name": "chrome-ext-maltasupermarket.com",
    "version": "0.0.9",
    "description": "Chrome extension to enchance the maltasupermarket.com experience",
    "main": "src/index.js",
    "repository": "git@github.com:frg/chrome-ext-maltasupermarket.git",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-preset-env": "^1.6.1",
        "copy-webpack-plugin": "^9.0.0",
        "eslint": "^7.27.0",
        "eslint-plugin-react": "^7.1.0",
        "nodemon": "^2.0.7",
        "rimraf": "^3.0.2",
        "webpack": "^5.37.1",
        "webpack-cli": "^4.7.0"
    },
    "scripts": {
        "start": "nodemon --exec yarn build",
        "prebuild": "rimraf dist",
        "build": "webpack"
    },
    "nodemonConfig": {
        "ignore": [
            "dist/",
            "node_modules"
        ],
        "execMap": {
            "js": "node"
        },
        "ext": "js html css",
        "verbose": true
    },
    "dependencies": {
        "arrive": "^2.4.1"
    }
}
